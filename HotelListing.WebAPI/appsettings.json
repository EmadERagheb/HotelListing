{
  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Information",
  //    "Microsoft.AspNetCore": "Warning"
  //  }
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "./logs/log-.txt",
            "rollingInterval": "Day"
          }
        },
        {
          "Name": "Seq",
          "Args": {
            "serverUrl": "http://localhost:5341/"
            //"apiKey": "yeEZyL3SMcxEKUijBjN",
            //"controlLevelSwitch": "$controlSwitch"
          }
        }
      ]
    
  },

  "AllowedHosts": "*"
}
